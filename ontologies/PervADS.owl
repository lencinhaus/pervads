    <!ENTITY PervADS "http://www.pervads.com/PervADS.owl#" >
    <!ENTITY owl2xml "http://www.w3.org/2006/12/owl2-xml#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY www "http://www.pervads.comshakespeare_cafe_pervads.owl#" >
    <!ENTITY shakespeare_cafe_pervads "http://www.pervads.com/shakespeare_cafe_pervads.owl#" >
    <!ENTITY contextModelOntology "http://www.dei.polimi.it/dbgroup/kid/contextModelOntology.owl#" >
]>


<rdf:RDF xmlns="http://www.pervads.com/PervADS.owl#"
     xml:base="http://www.pervads.com/PervADS.owl"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:contextModelOntology="http://www.dei.polimi.it/dbgroup/kid/contextModelOntology.owl#"
     xmlns:owl2xml="http://www.w3.org/2006/12/owl2-xml#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:www="http://www.pervads.comshakespeare_cafe_pervads.owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:shakespeare_cafe_pervads="http://www.pervads.com/shakespeare_cafe_pervads.owl#"
     xmlns:PervADS="http://www.pervads.com/PervADS.owl#">
    <owl:Ontology rdf:about="">
        <dc:date rdf:datatype="&xsd;date">2010-04-29</dc:date>
        <dc:date rdf:datatype="&xsd;date">29-04-2010</dc:date>
        <dc:creator rdf:datatype="&xsd;string"
            >Giorgio Orsi (orsi at elet dot polimi dot it).
Dipartimento di Elettronica e Informazione.
Politecnico di Milano.
Via Ponzio 34/5, 20133 Milan (Italy).</dc:creator>
        <dc:contributor rdf:datatype="&xsd;string"
            >Lorenzo Carrara (lorecarra at gmail dot com).
Dipartimento di Elettronica e Informazione.
Politecnico di Milano.
Via Ponzio 34/5, 20133 Milan (Italy).</dc:contributor>
        <dc:title rdf:datatype="&xsd;string"
            >The PervADS Ontology</dc:title>
        <dc:publisher rdf:datatype="&xsd;anyURI"
            >http://www.pervads.com</dc:publisher>
        <owl:imports rdf:resource="http://www.dei.polimi.it/dbgroup/kid/contextModelOntology.owl"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:AnnotationProperty rdf:about="&dc;date"/>
    <owl:AnnotationProperty rdf:about="&dc;publisher"/>
    <owl:AnnotationProperty rdf:about="&dc;title"/>
    <owl:AnnotationProperty rdf:about="&dc;contributor"/>
    <owl:AnnotationProperty rdf:about="&dc;creator"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.pervads.com/PervADS.owl#advertiser -->

    <owl:ObjectProperty rdf:about="#advertiser">
        <rdfs:label rdf:datatype="&xsd;string">Advertiser</rdfs:label>
        <rdfs:label rdf:datatype="&xsd;string">Inserzionista</rdfs:label>
        <rdfs:range rdf:resource="#Organization"/>
        <rdfs:domain rdf:resource="#PervADS"/>
    </owl:ObjectProperty>
    


    <!-- http://www.pervads.com/PervADS.owl#advertises -->

    <owl:ObjectProperty rdf:about="#advertises">
        <rdfs:label rdf:datatype="&xsd;string">Advertises</rdfs:label>
        <rdfs:label rdf:datatype="&xsd;string">Reclamizza</rdfs:label>
        <rdfs:range rdf:resource="#Offer"/>
        <rdfs:domain rdf:resource="#PervADS"/>
    </owl:ObjectProperty>
    


    <!-- http://www.pervads.com/PervADS.owl#context -->

    <owl:ObjectProperty rdf:about="#context">
        <rdfs:range rdf:resource="&contextModelOntology;Context"/>
        <rdfs:domain rdf:resource="#PervADS"/>
    </owl:ObjectProperty>
    


    <!-- http://www.pervads.com/PervADS.owl#isAdvertisedBy -->

    <owl:ObjectProperty rdf:about="#isAdvertisedBy">
        <rdfs:label rdf:datatype="&xsd;string"
            >is advertised by</rdfs:label>
        <rdfs:label rdf:datatype="&xsd;string"
            >&#232; reclamizzato da</rdfs:label>
        <owl:inverseOf rdf:resource="#advertises"/>
    </owl:ObjectProperty>
    


    <!-- http://www.pervads.com/PervADS.owl#item -->

    <owl:ObjectProperty rdf:about="#item">
        <rdfs:label rdf:datatype="&xsd;string">elemento</rdfs:label>
        <rdfs:label rdf:datatype="&xsd;string">item</rdfs:label>
        <rdfs:domain rdf:resource="#GoodServiceComposition"/>
        <rdfs:range>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#item"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="#Good"/>
                            <rdf:Description rdf:about="#Service"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://www.pervads.com/PervADS.owl#offers -->

    <owl:ObjectProperty rdf:about="#offers">
        <rdfs:label rdf:datatype="&xsd;string">offers</rdfs:label>
        <rdfs:label rdf:datatype="&xsd;string">offre</rdfs:label>
        <rdfs:range rdf:resource="#GoodServiceComposition"/>
        <rdfs:domain rdf:resource="#Offer"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.pervads.com/PervADS.owl#coupon -->

    <owl:DatatypeProperty rdf:about="#coupon">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">buono</rdfs:label>
        <rdfs:label rdf:datatype="&xsd;string">coupon</rdfs:label>
        <rdfs:domain rdf:resource="#Offer"/>
        <rdfs:range rdf:resource="&xsd;anyURI"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.pervads.com/PervADS.owl#currency -->

    <owl:DatatypeProperty rdf:about="#currency">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">currency</rdfs:label>
        <rdfs:label rdf:datatype="&xsd;string">valuta</rdfs:label>
        <rdfs:domain rdf:resource="#SpecialPrice"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.pervads.com/PervADS.owl#discount -->

    <owl:DatatypeProperty rdf:about="#discount">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">discount</rdfs:label>
        <rdfs:label rdf:datatype="&xsd;string">sconto</rdfs:label>
        <rdfs:domain rdf:resource="#Discount"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.pervads.com/PervADS.owl#itemDescription -->

    <owl:DatatypeProperty rdf:about="#itemDescription">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string"
            >descrizione dell&#39;elemento</rdfs:label>
        <rdfs:label rdf:datatype="&xsd;string"
            >item description</rdfs:label>
        <rdfs:domain rdf:resource="#Good"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.pervads.com/PervADS.owl#itemName -->

    <owl:DatatypeProperty rdf:about="#itemName">
        <rdfs:label rdf:datatype="&xsd;string">item&#39;s name</rdfs:label>
        <rdfs:label rdf:datatype="&xsd;string"
            >nome dell&#39;elemento</rdfs:label>
        <rdfs:domain rdf:resource="#Good"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.pervads.com/PervADS.owl#orgAddress -->

    <owl:DatatypeProperty rdf:about="#orgAddress">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string"
            >indirizzo dell&#39;organizzazione</rdfs:label>
        <rdfs:label rdf:datatype="&xsd;string"
            >organization address</rdfs:label>
        <rdfs:domain rdf:resource="#Organization"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.pervads.com/PervADS.owl#orgCity -->

    <owl:DatatypeProperty rdf:about="#orgCity">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">citt&#224;</rdfs:label>
        <rdfs:label rdf:datatype="&xsd;string">city</rdfs:label>
        <rdfs:domain rdf:resource="#Organization"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.pervads.com/PervADS.owl#orgEmail -->

    <owl:DatatypeProperty rdf:about="#orgEmail">
        <rdfs:domain rdf:resource="#Organization"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.pervads.com/PervADS.owl#orgName -->

    <owl:DatatypeProperty rdf:about="#orgName">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string"
            >nome dell&#39;organizzazione</rdfs:label>
        <rdfs:label rdf:datatype="&xsd;string"
            >organization&#39;s name</rdfs:label>
        <rdfs:domain rdf:resource="#Organization"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.pervads.com/PervADS.owl#orgWebsite -->

    <owl:DatatypeProperty rdf:about="#orgWebsite">
        <rdfs:domain rdf:resource="#Organization"/>
        <rdfs:range rdf:resource="&xsd;anyURI"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.pervads.com/PervADS.owl#orgZIP -->

    <owl:DatatypeProperty rdf:about="#orgZIP">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">CAP</rdfs:label>
        <rdfs:label rdf:datatype="&xsd;string">ZIP</rdfs:label>
        <rdfs:domain rdf:resource="#Organization"/>
        <rdfs:range rdf:resource="&xsd;int"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.pervads.com/PervADS.owl#orgZone -->

    <owl:DatatypeProperty rdf:about="#orgZone">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string"
            >city zone (e.g., neighborhood)</rdfs:label>
        <rdfs:label rdf:datatype="&xsd;string"
            >zona dell&#39;organizzazione (es. quartiere)</rdfs:label>
        <rdfs:domain rdf:resource="#Organization"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.pervads.com/PervADS.owl#price -->

    <owl:DatatypeProperty rdf:about="#price">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">prezzo</rdfs:label>
        <rdfs:label rdf:datatype="&xsd;string">price</rdfs:label>
        <rdfs:domain rdf:resource="#SpecialPrice"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.pervads.com/PervADS.owl#serviceDescription -->

    <owl:DatatypeProperty rdf:about="#serviceDescription">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string"
            >descrizione del servizio</rdfs:label>
        <rdfs:label rdf:datatype="&xsd;string"
            >service descriptin</rdfs:label>
        <rdfs:domain rdf:resource="#Service"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.pervads.com/PervADS.owl#serviceName -->

    <owl:DatatypeProperty rdf:about="#serviceName">
        <rdfs:label rdf:datatype="&xsd;string"
            >nome del servizio</rdfs:label>
        <rdfs:label rdf:datatype="&xsd;string">service&#39;s name</rdfs:label>
        <rdfs:domain rdf:resource="#Service"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.pervads.com/PervADS.owl#tags -->

    <owl:DatatypeProperty rdf:about="#tags">
        <rdfs:domain rdf:resource="#PervADS"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.pervads.com/PervADS.owl#validFrom -->

    <owl:DatatypeProperty rdf:about="#validFrom">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">valid from</rdfs:label>
        <rdfs:label rdf:datatype="&xsd;string">valido dal</rdfs:label>
        <rdfs:domain rdf:resource="#Offer"/>
        <rdfs:range rdf:resource="&xsd;date"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.pervads.com/PervADS.owl#validUntil -->

    <owl:DatatypeProperty rdf:about="#validUntil">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">valid until</rdfs:label>
        <rdfs:label rdf:datatype="&xsd;string">valido fino</rdfs:label>
        <rdfs:domain rdf:resource="#Offer"/>
        <rdfs:range rdf:resource="&xsd;date"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.dei.polimi.it/dbgroup/kid/contextModelOntology.owl#Context -->

    <owl:Class rdf:about="&contextModelOntology;Context"/>
    


    <!-- http://www.pervads.com/PervADS.owl#Discount -->

    <owl:Class rdf:about="#Discount">
        <rdfs:label rdf:datatype="&xsd;string">Discount</rdfs:label>
        <rdfs:label rdf:datatype="&xsd;string">Sconto</rdfs:label>
        <rdfs:subClassOf rdf:resource="#Offer"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#discount"/>
                <owl:someValuesFrom rdf:resource="&xsd;float"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.pervads.com/PervADS.owl#Good -->

    <owl:Class rdf:about="#Good">
        <rdfs:label rdf:datatype="&xsd;string">Bene</rdfs:label>
        <rdfs:label rdf:datatype="&xsd;string">Good</rdfs:label>
        <rdfs:subClassOf rdf:resource="&owl;Thing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#itemName"/>
                <owl:someValuesFrom rdf:resource="&xsd;string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#itemDescription"/>
                <owl:someValuesFrom rdf:resource="&xsd;string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.pervads.com/PervADS.owl#GoodServiceComposition -->

    <owl:Class rdf:about="#GoodServiceComposition">
        <rdfs:label rdf:datatype="&xsd;string"
            >Composizione di Beni e Servizi</rdfs:label>
        <rdfs:label rdf:datatype="&xsd;string"
            >Good and Service Composition</rdfs:label>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#item"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="#Good"/>
                            <rdf:Description rdf:about="#Service"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.pervads.com/PervADS.owl#Offer -->

    <owl:Class rdf:about="#Offer">
        <rdfs:label rdf:datatype="&xsd;string">Offer</rdfs:label>
        <rdfs:label rdf:datatype="&xsd;string">Offerta</rdfs:label>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#offers"/>
                <owl:someValuesFrom rdf:resource="#GoodServiceComposition"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#isAdvertisedBy"/>
                <owl:someValuesFrom rdf:resource="#PervADS"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#validUntil"/>
                <owl:someValuesFrom rdf:resource="&xsd;date"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#coupon"/>
                <owl:someValuesFrom rdf:resource="&xsd;anyURI"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#validFrom"/>
                <owl:someValuesFrom rdf:resource="&xsd;date"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.pervads.com/PervADS.owl#Organization -->

    <owl:Class rdf:about="#Organization">
        <rdfs:label rdf:datatype="&xsd;string">Organization</rdfs:label>
        <rdfs:label rdf:datatype="&xsd;string">Organizzazione</rdfs:label>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#orgCity"/>
                <owl:someValuesFrom rdf:resource="&xsd;string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#orgEmail"/>
                <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
                <owl:onDataRange rdf:resource="&xsd;string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#orgName"/>
                <owl:someValuesFrom rdf:resource="&xsd;string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#orgZone"/>
                <owl:someValuesFrom rdf:resource="&xsd;string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#orgAddress"/>
                <owl:someValuesFrom rdf:resource="&xsd;string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#orgZIP"/>
                <owl:someValuesFrom rdf:resource="&xsd;int"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#orgWebsite"/>
                <owl:someValuesFrom rdf:resource="&xsd;anyURI"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.pervads.com/PervADS.owl#PervADS -->

    <owl:Class rdf:about="#PervADS">
        <rdfs:label rdf:datatype="&xsd;string"
            >Inserzione Pubblicitaria Pervasiva</rdfs:label>
        <rdfs:label rdf:datatype="&xsd;string"
            >Pervasive Advertisement</rdfs:label>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#advertises"/>
                <owl:someValuesFrom rdf:resource="#Offer"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#advertiser"/>
                <owl:someValuesFrom rdf:resource="#Organization"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#tags"/>
                <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
                <owl:onDataRange rdf:resource="&xsd;string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#context"/>
                <owl:someValuesFrom rdf:resource="&contextModelOntology;Context"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.pervads.com/PervADS.owl#Service -->

    <owl:Class rdf:about="#Service">
        <rdfs:label rdf:datatype="&xsd;string">Service</rdfs:label>
        <rdfs:label rdf:datatype="&xsd;string">Servizio</rdfs:label>
        <rdfs:subClassOf rdf:resource="&owl;Thing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#serviceDescription"/>
                <owl:someValuesFrom rdf:resource="&xsd;string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#serviceName"/>
                <owl:someValuesFrom rdf:resource="&xsd;string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.pervads.com/PervADS.owl#SpecialPrice -->

    <owl:Class rdf:about="#SpecialPrice">
        <rdfs:label rdf:datatype="&xsd;string">Prezzo Speciale</rdfs:label>
        <rdfs:label rdf:datatype="&xsd;string">Special Price</rdfs:label>
        <rdfs:subClassOf rdf:resource="#Offer"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#price"/>
                <owl:someValuesFrom rdf:resource="&xsd;float"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#currency"/>
                <owl:someValuesFrom rdf:resource="&xsd;string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.w3.org/2002/07/owl#Thing -->

    <owl:Class rdf:about="&owl;Thing"/>
    


