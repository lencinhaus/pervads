    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.pervads.com/shakespeare_cafe_pervads.owl#good1 -->

    <owl:Thing rdf:about="&shakespeare_cafe_pervads;good1"/>
    


    <!-- http://www.pervads.com/shakespeare_cafe_pervads.owl#gsc1 -->

    <owl:Thing rdf:about="&shakespeare_cafe_pervads;gsc1"/>
    


    <!-- http://www.pervads.com/shakespeare_cafe_pervads.owl#organization1 -->

    <owl:Thing rdf:about="&shakespeare_cafe_pervads;organization1"/>
    


    <!-- http://www.pervads.com/shakespeare_cafe_pervads.owl#pervads1 -->

